
@{
    ViewBag.Title = "Trang chủ";
}



<div class="panel-header panel-header-lg">
    <canvas id="bigDashboardChart"></canvas>
</div>
<div class="content">
    <div class="row">
        <div class="col-md-6">
            <div class="card">
                <div class="card-header">
                    
                    <h4 class="card-title"> View tin tức</h4>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        @Html.Action("GetNewsView" , "Home")
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-6">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title"> View dự án</h4>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        @Html.Action("GetProjectView", "Home")
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>