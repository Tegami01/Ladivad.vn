
@model IEnumerable<Model.DataModel.NewsDTO>


@foreach (var item in Model)
{
    <div class="col-md-6 col-sm-6 col-6">
        <div class="item-small">
            <div class="avarta"><a href="/Project/Details/@item.Url"><img src="~/uploads/news/@item.ImgMain" class="img-fluid" width="100%"></a></div>
            <div class="info">
                <h3>
                    <a href="/Project/Details/@item.Url">@item.Title</a>
                </h3>
            </div>
        </div>
    </div>
}


